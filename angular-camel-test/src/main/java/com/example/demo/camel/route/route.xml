<rests xmlns="http://camel.apache.org/schema/spring">
   <rest id="login">
      <post uri="/api/login">
      <process ref="loginProcessor"/>
         <to uri="http://localhost:8081/api/login?bridgeEndpoint=true"/>
      </post>
   </rest>
   
    <rest id="baseinfo">
      <post uri="/api/baseinfo">
      <process ref="baseinfoProcessor"/>
         <to uri="http://localhost:8081/api/baseinfo?bridgeEndpoint=true"/>
      </post>
   </rest>
 </rests>
 </rests>